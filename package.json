{
  "name": "angular-workbox",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration=production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve:pwa": "npm run build:pwa && npm run start:pwa",
    "start:pwa": "http-server dist -d -c-1 -a 0.0.0.0 --proxy http://localhost:4200 --port 4200",
    "build:pwa": "npm run build && webpack --config ./service-worker/webpack.config.js && node ./service-worker/workbox-build-inject.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.5",
    "@angular/common": "~11.2.5",
    "@angular/compiler": "~11.2.5",
    "@angular/core": "~11.2.5",
    "@angular/forms": "~11.2.5",
    "@angular/platform-browser": "~11.2.5",
    "@angular/platform-browser-dynamic": "~11.2.5",
    "@angular/router": "~11.2.5",
    "ngx-toastr": "^13.2.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3",
    "register-service-worker": "^1.7.2",
    "workbox-background-sync": "6.1.2",
    "workbox-broadcast-update": "6.1.2",
    "workbox-cacheable-response": "6.1.2",
    "workbox-core": "6.1.2",
    "workbox-expiration": "6.1.2",
    "workbox-navigation-preload": "6.1.2",
    "workbox-precaching": "6.1.2",
    "workbox-recipes": "^6.1.2",
    "workbox-routing": "6.1.2",
    "workbox-strategies": "6.1.2",
    "workbox-window": "6.1.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.3",
    "@angular/cli": "~11.2.4",
    "@angular/compiler-cli": "~11.2.5",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5",
    "http-server": "^0.12.3",
    "ts-loader": "6.2.1",
    "webpack-cli": "^4.5.0",
    "workbox-build": "5.0.0"
  }
